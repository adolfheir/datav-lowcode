{
  // "extends": "./node_modules/@ihouqi/cli-config/dist/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "importHelpers": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "paths": {
      "@public/*": ["./public/*"],
      "@app/*": ["./src/app/*"],
      "@assets/*": ["./src/assets/*"],
      "@model/*": ["./src/model/*"],
      "@common/*": ["./src/common/*"],
      "@constants/*": ["./src/common/constants/*"],
      "@utils/*": ["./src/common/utils/*"],
      "@hooks/*": ["./src/common/hooks/*"],
      "@components/*": ["./src/common/components/*"],
      "@materia/*": ["./src/common/materia/*"],
      "@pages/*": ["./src/pages/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*", "config/**/*", ],
  "exclude": ["node_modules", "lib", "es", "dist", "typings", "**/__test__", "test", "docs", "tests", "*.js"]
}
